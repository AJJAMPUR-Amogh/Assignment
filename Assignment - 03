    {
      "cell_type": "code",
      "source": [
        "\n",
        "text = \"Welcome to iNeuron, You are a part of FSDS Bootcamp 2 in iNeuron. I hope you are enjoying the course by iNeuron\"\n",
        "text = text.replace(',', '') \n",
        "text = text.replace('.', '') \n",
        "count = 0\n",
        "words = text.split(' ')\n",
        "loop = 0\n",
        "print(words)\n",
        "for word in words :\n",
        "   loop = loop +1 \n",
        "   if word == 'iNeuron' :\n",
        "    count = count + 1\n",
        "\n",
        "print(f' The number of times iNeuron appeared is {count}')\n",
        "print(loop)"
