      "cell_type": "code",
      "source": [
        "words = ['3253', 'datascience','MACHINELEARNING','$30']\n",
        "allcaps =[]\n",
        "numeric = []\n",
        "lower = []\n",
        "al = []\n",
        "upper = []\n",
        "for i in words :\n",
        "  if i.isnumeric or i.isdecimal() :\n",
        "    allcaps.append(i)\n",
        "  elif i.islower() :\n",
        "    lower.append(i)\n",
        "  elif i.isupper() :\n",
        "    upper.append(i) \n",
        "  elif i.isalpha() :\n",
        "    al.append(i)\n",
        "\n",
